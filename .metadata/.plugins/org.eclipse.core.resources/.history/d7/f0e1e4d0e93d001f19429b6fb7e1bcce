package Ch_2_program;

public class VarDemo {
	public static void main(String[] args) {

		int age = 20;
		System.out.println(age);

		float a = 25.01f;
		System.out.println(a);

		double price = 120.90;
		System.out.println(price);

		char gender = 'm';
		System.out.println(gender);

		boolean pass = true;
		System.out.println(pass);
	}

}  
