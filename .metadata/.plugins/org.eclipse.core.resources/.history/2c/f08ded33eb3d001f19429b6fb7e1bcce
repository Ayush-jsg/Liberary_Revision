package ch_3_program;

// write java program on Arthematic program

public class Demo {
	public static void main(String[] args) {

		int a = 10;
		int b = 2;

		System.out.println(a + b);
		System.out.println(a - b);
		System.out.println(a * b);
		System.out.println(a / b);
		System.out.println(a % b);
	}

}
